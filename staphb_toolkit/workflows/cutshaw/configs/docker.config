docker.enabled = true

includeConfig 'docker_containers.config'

process {
  withName:preProcess{
    container = fastqc_container
  }
  withName:spades{
    container = spades_container
  }
  withName:quast{
    container = quast_container
  }
  withName:fastani{
    container = fastani_container
  }
  withName:cfsan_snp{
    container = cfsan_container
  }
}
